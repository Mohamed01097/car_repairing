<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <menuitem name="Configuration"
                  id="menu_configuration"
                  parent="car_repair_industry.menu_main_car_repair"
                  sequence="80"/>

        <record model="ir.ui.view" id="view_technician_form">
            <field name="name">technician.form.view</field>
            <field name="model">technician</field>
            <field name="arch" type="xml">
                <form>
                    <sheet string="Technician">
                        <group>
                            <group>
                                <field name="name"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record model="ir.ui.view" id="view_technician_tree">
            <field name="name">technician.tree.view</field>
            <field name="model">technician</field>
            <field name="arch" type="xml">
                <list>
                    <field name="name"/>
                </list>
            </field>
        </record>
        <record model="ir.actions.act_window" id="action_technician">
            <field name="name">Technician</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">technician</field>
            <field name="view_mode">list,form</field>
        </record>

<!--        <menuitem name="Technician"-->
<!--                  action="action_technician"-->
<!--                  id="menu_technician"-->
<!--                  parent="car_repair_industry.menu_configuration"-->
<!--                  />-->

    </data>
</odoo>